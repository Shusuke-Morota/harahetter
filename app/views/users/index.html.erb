<h1>Users#index</h1>
<p>Find me in app/views/users/index.html.erb</p>



<div class="container mt-5">
	<div class="row">
		<% @users.each do |user| %>
		<div class="col-3">
			<div class="card">
				<div class="card-body">
					<div class="card-content">
						<div class="card-image">
							<figure class="image">
								<%= link_to user_path(user) do %>
								<%= attachment_image_tag user, :profile_image, :fill, 260, 280, fallback: "no-image.png", class:"img-fluid" %>
								<% end %>
							</figure>
						</div>
					</div>
				</div>
				<div class="card-content">
					<div class="card-body">
						<div class="media">
							<div class="media-content">
								<div class="index-user-name">
									<%= link_to user.name, user_path(user) %>
								</div>
							</div>
						</div>
					</div>
				</div>
			</div>
		</div>
		<% end %>
	</div>
</div>


